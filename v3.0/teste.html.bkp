    <div id="divArquivoDeOrigem">
        <input type="file" id="fileArquivo">
    </div>
    <div id="divContainerPrincipal" class="container">
        <fieldset id="fldGrupo">
            <label id="lblGrupo">
                <label class="lblDescricao">Grupos:</label>
